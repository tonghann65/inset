export default function RootLayout({ children }) {
  return (
    <html lang="en">
      <head>
       <!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-Y5C5C7S6XH"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-Y5C5C7S6XH');
</script>
      </head>
      <body>{children}</body>
    </html>
  );
}
